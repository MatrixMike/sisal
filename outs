DEFINE MAIN
TYPE DOUBLE = DOUBLE_REAL;
TYPE THREED = ARRAY [ARRAY [ARRAY [DOUBLE]]]
FUNCTION MAIN (N,M:INTEGER;Y:THREED RETURNS INTEGER,INTEGER)
  LET
X := 1;Y,Z := LET
X := 2;Y := 3 IN
X + Y + X * Y,X * Y + X * Y
END LET IN
  X * Y + Z,X * Y + Z
  END LET
END FUNCTION
